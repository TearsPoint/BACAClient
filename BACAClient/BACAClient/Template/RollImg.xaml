<UserControl x:Class="BACAClient.Template.RollImg"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:BACAClient.Template"
             mc:Ignorable="d"  Width="960"  Height="350" HorizontalAlignment="Center">
    <Grid>
        <Canvas Name="canvas_board"  Margin="0,0,0,0" VerticalAlignment="Top" >
            <Image Name="image1"
                Canvas.Left="0" Canvas.Top="0" Stretch="UniformToFill"  Width="960"  Height="350"  Source="pack://application:,,,/BACAClient;component/Images/rollimg/img1.png"/>
            <Image Name="image2"
                Canvas.Left="0" Canvas.Top="0" Stretch="UniformToFill" Width="960"  Height="350"  />
        </Canvas>
    </Grid>

</UserControl>
