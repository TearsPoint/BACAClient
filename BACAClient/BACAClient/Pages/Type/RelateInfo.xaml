<base:BasePage  x:Class="BACAClient.Pages.Type.RelateInfo" 
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"    
        xmlns:base="clr-namespace:BACAClient.Header"   
                base:BasePage.Loaded="_Loaded" base:BasePage.SizeChanged="_SizeChanged" Page.Title="RelateInfo">
    <ScrollViewer Style="{DynamicResource ScrollViewerStyle}" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation">
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition RowDefinition.Height="auto" />
                <RowDefinition RowDefinition.Height="auto" />
                <RowDefinition RowDefinition.Height="auto" />
            </Grid.RowDefinitions>
            <Grid Name="Page1" />
            <StackPanel Name="List" Grid.Row="1" HorizontalAlignment="Center" />
            <Grid Name="Page2" Grid.Row="2" />
        </Grid>
    </ScrollViewer>
</base:BasePage>
 

 
